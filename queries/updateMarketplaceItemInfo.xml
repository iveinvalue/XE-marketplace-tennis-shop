<query id="updateMarketplaceItemInfo" action="update">
    <tables>
        <table name="marketplace_items" />
    </tables>
    <columns>
        <column name="product_name" var="product_name" />
		<column name="product_name2" var="product_name2" />
        <column name="original_price" var="original_price" />
        <column name="price" var="price" />
        <column name="item_condition" var="item_condition" />
        <column name="used_month" var="used_month" />
        <column name="priority_area" var="priority_area" />
        <column name="delivery" var="delivery" />
        <column name="direct_dealing" var="direct_dealing" />
        <column name="safe_dealing" var="safe_dealing" />
		<column name="seller_contact" var="seller_contact" />
		<column name="size1" var="size1" />
		<column name="size2" var="size2" />
		
		<column name="string1" var="string1"/>
		<column name="string2" var="string2"/>
		
		<column name="frame1" var="frame1"/>
		<column name="frame2" var="frame2"/>
		<column name="frame3" var="frame3"/>
	
		<column name="length" var="length"/>
		<column name="g_size" var="g_size"/>
	</columns>
    <conditions>
        <condition operation="equal" column="document_srl" var="document_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
